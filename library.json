        "+<plug-and-trust/sss/src/keystore/keystore_pc.c>",
        "-<plug-and-trust/hostlib/hostLib/libCommon/nxScp/>",
        "-<plug-and-trust/hostlib/hostLib/libCommon/nxScp/*>",
        "-<plug-and-trust/hostlib/hostLib/libCommon/nxScp/nxScp03_Com.c>"
      ],
      "flags": [
        "-Isrc/port",
        "-Isrc/plug-and-trust/hostlib/hostLib/inc",
        "-Isrc/plug-and-trust/hostlib/hostLib/platform/inc",
        "-Isrc/plug-and-trust/hostlib/hostLib/libCommon/infra",
        "-Isrc/plug-and-trust/hostlib/hostLib/libCommon/log",
        "-Isrc/plug-and-trust/hostlib/hostLib/libCommon/hostCrypto",
